{
  "name": "youtube-webos",
  "version": "0.3.1",
  "description": "Youtube App without ADs",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "package": "ares-package -n dist",
    "deploy": "node tools/deploy.mjs",
    "launch": "ares-launch youtube.leanback.v4",
    "manifest": "node tools/gen-manifest.js youtube.leanback.v4.manifest.json",
    "version": "node tools/sync-version.js && git add assets/appinfo.json",
    "prepare": "cd .. && husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webosbrew/youtube-webos.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/webosbrew/youtube-webos/issues"
  },
  "homepage": "https://github.com/webosbrew/youtube-webos#readme",
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "core-js-pure": "^3.20.2",
    "regenerator-runtime": "^0.13.9",
    "tiny-sha256": "^1.0.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@webosose/ares-cli": "^2.2.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-polyfill-corejs3": "^0.5.0",
    "babel-plugin-polyfill-regenerator": "^0.3.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  }
}